<%= link_to 'Submit New Question', new_question_path %>

<h1>List of Questions</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>

  <% @questions.each do |question| %>
    <tr>
      <td><%= question.title %></td>
      <td><%= question.description %></td>
      <td><%= link_to 'Show', question_path(question)  %></td>
      <td><%= link_to 'Edit', edit_question_path(question.id) %></td>
      <td><%= link_to "Delete", question_path(question), method: :delete %> </td>
    </tr>
  <% end %>
</table>


