<h2>Post New Questions</h2>
<%= flash[:notice] %>

<%= form_for @question do |f| %>

  <p>
    <%= f.label :title %><br>
    <%= f.text_area :title %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', questions_path %>
