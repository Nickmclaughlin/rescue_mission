<%= link_to 'Submit New Question', new_question_path %>

<h1>List of Questions</h1>

<table>
  <tr>
    <th>Title of Question</th>
    <th>Description</th>
  </tr>

  <% @questions.each do |question| %>
    <tr>
      <td><%= link_to question.title, question_path(question)  %></td>
      <td><%= question.description %></td>
    </tr>
  <% end %>
</table>


