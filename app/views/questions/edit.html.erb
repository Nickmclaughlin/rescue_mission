<%= form_for @question do |f| %>

<%= flash[:notice] %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_area :title %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to "Delete", question_path(@question), method: :delete %><br>
<%= link_to "Go Back", question_path(@question) %>


